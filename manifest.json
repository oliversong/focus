{
  "name": "Focus",
  "version": "1.3",
  "manifest_version": 3,
  "description": "Extension to stop procrastination.",
  "background": {
    "service_worker": "background.js"
  },
  "icons":{
    "16":"icon2.png",
    "48":"icon48.png",
    "128":"icon128.png"
  },
  "permissions": [
  ],
  "action": {
    "name": "Focus",
    "default_icon": "icon.png"
  },
  "content_scripts":[{
    "matches":["http://*/*","https://*/*"],
    "exclude_matches": ["https://*.google.com/*"],
    "js":["contentscript.js"]
  }],
  "web_accessible_resources": [{
    "resources": ["script.js","shoop.css"],
    "matches":["http://*/*","https://*/*"]
  }]
}
