{
  "name": "Rage Mode",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Extension to stop procrastination.",
  "background": { "scripts": ["background.js"] },
  "icons":{
    "16":"icon2.png",
    "48":"icon48.png",
    "128":"icon128.png"
  },
  "permissions": [
    "cookies",
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
    "name": "Rage Mode",
    "default_icon": "icon.png"
  },
  "content_scripts":[{
    "matches":["http://*/*","https://*/*"],
    "js":["contentscript.js"]
  }],
  "web_accessible_resources": ["script.js","shoop.css"]
}