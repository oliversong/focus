{
  "name": "Focus",
  "version": "1.2",
  "manifest_version": 3,
  "description": "Extension to stop procrastination.",
  "background": { "scripts": ["background.js"] },
  "icons":{
    "16":"icon2.png",
    "48":"icon48.png",
    "128":"icon128.png"
  },
  "permissions": [
  ],
  "browser_action": {
    "name": "Focus",
    "default_icon": "icon.png"
  },
  "content_scripts":[{
    "matches":["http://*/*","https://*/*"],
    "js":["contentscript.js"]
  }],
  "web_accessible_resources": ["script.js","shoop.css"]
}
